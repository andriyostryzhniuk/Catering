<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ostryzhniuk.andriy.catering.main.window.MainWindowController">
    <content>
        <BorderPane>
            <top>
                <MenuBar fx:id="menuBar" BorderPane.alignment="TOP_LEFT">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Close"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <center>
                <GridPane fx:id="mainGridPane" prefHeight="573.0" prefWidth="1298.0" BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="170.0"/>
                        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="1180.0"/>
                        <ColumnConstraints hgrow="ALWAYS" maxWidth="30.0" minWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.0" minWidth="0.0" prefWidth="9.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="30.0" minHeight="10.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="557.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="120.0" minHeight="70.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <GridPane prefHeight="517.0" prefWidth="216.0" GridPane.rowIndex="1">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="170.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="328.0" prefHeight="328.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Button mnemonicParsing="false" onAction="#initOrderView" prefHeight="25.0"
                                        prefWidth="150.0" text="Замовлення">
                                    <GridPane.margin>
                                        <Insets left="10.0"/>
                                    </GridPane.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#initClientView" prefHeight="25.0"
                                        prefWidth="150.0" text="Клієнти" GridPane.rowIndex="3">
                                    <GridPane.margin>
                                        <Insets left="10.0"/>
                                    </GridPane.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#initMenuView" prefHeight="25.0"
                                        prefWidth="150.0" text="Меню" GridPane.rowIndex="2">
                                    <GridPane.margin>
                                        <Insets left="10.0"/>
                                    </GridPane.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#onActionOrderingButton" prefHeight="25.0"
                                        prefWidth="150.0" text="Оформити замовлення" GridPane.rowIndex="1">
                                    <GridPane.margin>
                                        <Insets left="10.0"/>
                                    </GridPane.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#onActionDebtorsButton" prefHeight="25.0"
                                        prefWidth="150.0" text="Боржники" GridPane.rowIndex="4">
                                    <GridPane.margin>
                                        <Insets left="10.0"/>
                                    </GridPane.margin>
                                </Button>
                            </children>
                        </GridPane>
                    </children>
                </GridPane>
            </center>
        </BorderPane>
    </content>
</ScrollPane>
